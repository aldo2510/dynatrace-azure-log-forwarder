{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[1.*, 3.0.0)"
  },
  "extensions": {
        "eventHubs": {
          "batchCheckpointFrequency": 10,
          "eventProcessorOptions": {
                "maxBatchSize": 100,
                "prefetchCount": 2000
            },
            "initialOffsetOptions": {
                "type": "fromEnd",
                "enqueuedTimeUtc": ""
            }
        }
    }
}
